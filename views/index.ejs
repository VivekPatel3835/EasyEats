<% include partials/_head %>

<main>
  <% include partials/_header %>
  <!-- Slider Section -->

    <section class="slider">
      <div id="bootstrap-touch-slider" class="carousel bs-slider fade  control-round indicators-line" data-ride="carousel" data-pause="hover" data-interval="5000" >


            <!-- <ol class="carousel-indicators">
                <li data-target="#bootstrap-touch-slider" data-slide-to="0" class="active"></li>
                <li data-target="#bootstrap-touch-slider" data-slide-to="1"></li>
                <li data-target="#bootstrap-touch-slider" data-slide-to="2"></li>
            </ol>
 -->
            <!-- Wrapper For Slides -->
            <div class="carousel-inner" role="listbox">

                <!-- Third Slide -->
                <div class="item active">

                    <!-- Slide Background -->
                    <img src="http://lorempixel.com/940/680/food/2" alt="Touch Slider"  class="slide-image"/>
                    <div class="bs-slider-overlay"></div>

                    <div class="container">
                        <div class="row">
                            <!-- Slide Text Layer -->
                            <div class="slide-text slide_style_left">
                                <h1 data-animation="animated zoomInRight">Delicious Menu</h1>
                                <p data-animation="animated fadeInLeft">Mouth Watering Dishes</p>
                                <a href="/" target="_blank" class="btn btn-default" data-animation="animated fadeInLeft">VIEW MENU</a>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- End of Slide -->

                <!-- Second Slide -->
                <div class="item">

                    <!-- Slide Background -->
                    <img src="http://lorempixel.com/940/680/food/3" alt="Touch Slider"  class="slide-image"/>
                    <div class="bs-slider-overlay"></div>
                    <!-- Slide Text Layer -->
                    <div class="slide-text slide_style_center">
                        <h1 data-animation="animated flipInX">Delicious Menu</h1>
                        <p data-animation="animated lightSpeedIn">Mouth Watering Dishes</p>
                        <a href="/" target="_blank" class="btn btn-default" data-animation="animated fadeInUp">VIEW MENU</a>

                    </div>
                </div>
                <!-- End of Slide -->

                <!-- Third Slide -->
                <div class="item">

                    <!-- Slide Background -->
                    <img src="http://lorempixel.com/940/680/food/1" alt="Touch Slider"  class="slide-image"/>
                    <div class="bs-slider-overlay"></div>
                    <!-- Slide Text Layer -->
                    <div class="slide-text slide_style_right">
                        <h1 data-animation="animated zoomInLeft">Delicious Menu</h1>
                        <p data-animation="animated fadeInRight">Mouth Watering Dishes</p>
                        <a href="/" target="_blank" class="btn btn-default" data-animation="animated fadeInLeft">VIEW MENU</a>

                    </div>
                </div>
                <!-- End of Slide -->


            </div><!-- End of Wrapper For Slides -->

            <!-- Left Control -->
            <a class="left carousel-control" href="#bootstrap-touch-slider" role="button" data-slide="prev">
                <span class="fa fa-angle-left" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
            </a>

            <!-- Right Control -->
            <a class="right carousel-control" href="#bootstrap-touch-slider" role="button" data-slide="next">
                <span class="fa fa-angle-right" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </a>

        </div> <!-- End  bootstrap-touch-slider Slider -->

    </section>

    <!-- How to use section -->

    <section class="how_to_use">
      <h2 class="text-center how_to_order">How to Order?</h2>
      <div class="col-md-3  text-center">
        <i class="fa fa-map-marker" aria-hidden="true"></i>

        <p class="counter_seq">1</p>
        <p class="step_content">Choose your location</p>

      </div>

      <div class="col-md-3  text-center">
        <i class="fa fa-cutlery" aria-hidden="true"></i>
        <p class="counter_seq">2</p>
        <p class="step_content">Choose restaurant</p>
      </div>

      <div class="col-md-3  text-center">
        <i class="fa fa-cutlery" aria-hidden="true"></i>
        <p class="counter_seq">3</p>
        <p class="step_content">Make your order</p>
      </div>

      <div class="col-md-3  text-center">
          <i class="fa fa-truck" aria-hidden="true"></i>

        <p class="counter_seq">4</p>
        <p class="step_content">Food is on the way</p>
      </div>
    </section>


       <!-- Cherry image section-->

    <section class="second_section" id="about">
      <div class="container">
        <div class="row">
          <div class="col-lg-6 mx-auto text-center">
            <h2 class="section-heading text-white">We've got what you need!</h2>
            <hr class="light my-4">
            <p class="text-faded mb-4">Start Bootstrap has everything you need to get your new website up and running in no time! All of the templates and themes on Start Bootstrap are open source, free to download, and easy to use. No strings attached!</p>
            <a class="btn btn-light btn-xl js-scroll-trigger" href="#services">Get Started!</a>
          </div>
        </div>
      </div>
    </section>


    <!-- COUNTER section-->

    <section id="counter_threeup" class="counter_threeup lightbg sections">
